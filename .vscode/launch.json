{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Keys Bot",
			"program": "${workspaceFolder}/apps/keys/src/index.ts",
			"request": "launch",
			"skipFiles": ["<node_internals>/**"],
			"runtimeExecutable": "nodemon",
			"type": "node",
			"envFile": "${workspaceFolder}/apps/keys/.env",
			"outFiles": ["${workspaceFolder}/apps/keys/dist/**/*.js"]
		},
		{
			"name": "Launch Users Bot",
			"program": "${workspaceFolder}/apps/users/src/index.ts",
			"request": "launch",
			"skipFiles": ["<node_internals>/**"],
			"runtimeExecutable": "nodemon",
			"type": "node",
			"envFile": "${workspaceFolder}/apps/users/.env",
			"outFiles": ["${workspaceFolder}/apps/users/dist/**/*.js"]
		}
	]
}
