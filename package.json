{
  "name": "vpn-bot",
  "version": "1.0.0",
  "description": "Simple Telegram bot for Dagon VPN management",
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run prisma:build && tsc",
    "build:receiver": "tsc -p ./tsconfig.receiver.json",
    "prisma:build": "prisma migrate deploy && prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prestart": "npm run build",
    "start": "node dist/main.js",
    "start:receiver": "node dist/services/receiver.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/temqua/vpn-bot.git"
  },
  "author": "Artem Nazarov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/temqua/vpn-bot/issues"
  },
  "homepage": "https://github.com/temqua/vpn-bot#readme",
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "ms": "^2.1.3",
    "node-telegram-bot-api": "^0.61.0",
    "pg": "^8.11.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@jest/globals": "^29.5.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.16.19",
    "@types/node-telegram-bot-api": "^0.57.6",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "typescript": "^4.9.5"
  }
}
